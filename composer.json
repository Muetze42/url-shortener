{
    "name": "norman-huth/url-shortener",
    "type": "project",
    "description": "A URL shortener created with Laravel and Laravel Nova.",
    "keywords": [
        "laravel",
        "framework",
        "url-shortener",
        "laravel-nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "hisorange/browser-detect": "^5.0",
        "jaybizzle/crawler-detect": "^1.2",
        "laravel/framework": "^10.33",
        "laravel/nova": "^4.32.2",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "norman-huth/nova-assets-versioning": "^1.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-medialibrary": "^10.15"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23",
        "laravel/dusk": "^7.11",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.6",
        "norman-huth/helpers-collection-laravel": "^1.1",
        "nunomaduro/collision": "^7.1",
        "phpunit/phpunit": "^10.4",
        "spatie/laravel-ignition": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "functions/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "functions/helpers-dev.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "./vendor/bin/pint",
            "@php artisan test"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "code-quality": [
            "./vendor/bin/pint",
            "phpmd app,database,config,routes text phpmd.xml"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
